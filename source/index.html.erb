<div class="main-container">
  <div class="card-call text-center">
    <h2>Vous avez un projet ?</h2>
    <!-- Button trigger modal -->
    <button type="button" class="btn-blue" data-toggle="modal" data-target="#formCallModal">
      Contactez-nous
    </button>
  </div>
  <!-- Modal -->
  <%= partial "layouts/partials/form-call-modal" %>
</div>

<div class="main-container padding-bottom">
  <div class="cards cards-small">
    <% data.prestations.each do |f, prestation| %>
      <div class="card-verti text-center">
        <div class="picto">
          <%= prestation.icon %>
        </div>
        <div class="card-verti-infos">
          <div>
            <h3><%= prestation.titre %></h3>
            <p><%= prestation.subtitle %></p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>

